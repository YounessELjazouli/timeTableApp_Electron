{
  "name": "my-app",
  "productName": "ofppt temara app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.1.3",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-export-table-to-excel": "^1.0.6",
    "react-router-dom": "^5.3.4",
    "react-scripts": "^5.0.1",
    "react-table": "^7.8.0",
    "reactjs-popup": "^2.0.5",
    "sqlite-electron": "^2.0.5",
    "sqlite3": "^5.1.2",
    "v8-compile-cache": "^2.3.0",
    "web-vitals": "^2.1.4"
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "devStart": "nodemon app.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=public/logo.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Gestion des emplois de temps \"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.75",
    "@electron-forge/maker-deb": "^6.0.0-beta.75",
    "@electron-forge/maker-rpm": "^6.0.0-beta.75",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.75",
    "@electron-forge/maker-zip": "^6.0.0-beta.75",
    "electron": "^21.2.0",
    "electron-builder": "^23.6.0",
    "nodemon": "^2.0.20"
  }
}
